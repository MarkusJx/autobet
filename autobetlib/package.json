{
    "name": "@autobet/autobetlib",
    "version": "1.4.1",
    "private": true,
    "description": "The autobet native library module",
    "main": "index.js",
    "scripts": {
        "test": "node test.js",
        "pretest": "npm run-script build",
        "test:dev": "node test.js",
        "pretest:dev": "npm run-script build:dev",
        "build": "cmake-js build",
        "build:dev": "cmake-js configure --debug && cmake --build build --config Debug",
        "postbuild:dev": "node prelaunch.js --dev",
        "postbuild": "node prelaunch.js",
        "clean": "cmake-js clean",
        "preinstall": "npm run conan_install",
        "conan_install:dev": "conan install . -s build_type=Debug",
        "conan_install": "conan install . --build missing"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MarkusJx/autobet.git"
    },
    "author": "MarkusJx",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/MarkusJx/autobet/issues"
    },
    "homepage": "https://github.com/MarkusJx/autobet#readme",
    "cmake-js": {
        "runtime": "electron",
        "arch": "x64",
        "runtimeVersion": "20.1.3"
    },
    "devDependencies": {
        "cmake-js": "^6.3.2",
        "node-addon-api": "^7.0.0",
        "segfault-handler": "^1.3.0"
    },
    "dependencies": {
        "@markusjx/n-api-tools": "^1.2.0",
        "get-current-line": "^6.6.0"
    }
}
