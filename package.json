{
    "name": "autobet",
    "version": "1.3.2",
    "description": "A GTA Online horse racing betting bot",
    "main": "out/main.bundled.js",
    "scripts": {
        "build": "cd autobetlib && npm run-script build",
        "build:preload": "webpack build --config app/preload/webpack.config.js --mode production",
        "build:renderer": "webpack build --config app/renderer/webpack.config.js --mode production",
        "build:main": "webpack build --config app/main/webpack.config.js --mode production",
        "postbuild": "npm run build:main && npm run build:preload && npm run build:renderer",
        "start": "electron .",
        "prestart": "npm run-script build",
        "test": "electron . --enableDevTools",
        "pack": "electron-builder",
        "prepack": "npm run-script build",
        "postinstall": "node install.js --downloadModel && cd autobetlib && npm install",
        "clean": "node install.js --clean && cd autobetlib && npm run-script clean",
        "clean_all": "cd autobetlib && npm run-script clean && cd .. && node install.js --clean_all",
        "testrun": "npm run postbuild && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MarkusJx/autobet.git"
    },
    "author": "MarkusJx",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/MarkusJx/autobet/issues"
    },
    "homepage": "https://github.com/MarkusJx/autobet#readme",
    "devDependencies": {
        "@babel/core": "^7.16.7",
        "@babel/plugin-transform-react-jsx": "^7.16.7",
        "@babel/preset-env": "^7.16.7",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@types/node": "^17.0.5",
        "@types/react-dom": "^17.0.11",
        "@types/uuid": "^8.3.1",
        "babel-loader": "^8.2.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "csp-html-webpack-plugin": "^5.1.0",
        "css-loader": "^6.5.1",
        "electron": "^16.0.5",
        "electron-builder": "^22.14.2",
        "html-loader": "^3.0.1",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.4.5",
        "node-loader": "^2.0.0",
        "sass": "^1.45.2",
        "sass-loader": "^12.4.0",
        "typescript": "^4.5.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "dependencies": {
        "@autobet/autobetlib": "file:autobetlib",
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@mui/icons-material": "^5.2.5",
        "@mui/material": "^5.2.6",
        "ace-builds": "^1.4.13",
        "corejs": "^1.0.0",
        "custom-electron-titlebar": "^3.2.10",
        "electron-store": "^8.0.1",
        "electron-updater": "^4.3.9",
        "electron-window-state": "^5.0.3",
        "open-sans-fonts": "^1.6.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "uuid": "^8.3.2",
        "vm2": "^3.9.4"
    },
    "build": {
        "appId": "markusjx.autobet",
        "icon": "resources/icon.png",
        "nsis": {
            "guid": "41a0f9e2-b869-421a-a98f-2ac0e8f8d02d",
            "oneClick": false,
            "artifactName": "${productName}-v${version}.${ext}",
            "license": "pack/license.txt",
            "installerLanguages": [
                "en_US"
            ],
            "installerIcon": "pack/icon.ico",
            "include": "pack/installer.nsh"
        },
        "files": [
            "ui/index.html",
            "ui/mountains.svg",
            "ui/style.min.css",
            "ui/err/*",
            "main.js",
            "out/**",
            "icon.png",
            "package.json",
            "LICENSE",
            "!**/unused/**",
            "!**.md",
            "!**/autobetlib/prelaunch.js",
            "!**/autobetlib/build",
            "!**/autobetlib/external",
            "!**/autobetlib/include",
            "!**/autobetlib/src",
            "!**/autobetlib/CMake**",
            "!**/autobetlib/cmake-build-*",
            "!**/autobetlib/conan*",
            "!**/autobetlib/graph_info.json"
        ],
        "extraResources": [
            {
                "from": "resources/data",
                "to": "data",
                "filter": [
                    "**/*",
                    "!*.md"
                ]
            },
            {
                "from": "resources/icon.png",
                "to": "icon.png"
            },
            {
                "from": "web",
                "to": "web",
                "filter": [
                    "**/*.min.*",
                    "**/index.html",
                    "**/favicon.ico"
                ]
            }
        ],
        "asar": true
    }
}
